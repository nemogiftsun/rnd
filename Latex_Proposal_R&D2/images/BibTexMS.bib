@ARTICLE{gps12,
  author = {Xian-Chun Bi; Jin-Chun Hu; Ji-Hong Zhu; Zeng-Qi Sun},
  title = {Position estimation for autonomous flight of unmanned helicopter
	based on low-cost dual GPS},
  journal = {Proceedings of the Second International Conference on Machine Learning
