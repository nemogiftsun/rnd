@ARTICLE{Peasgood05localizationof,
  author = {Mike Peasgood and Christopher Clark},
  title = {Localization of multiple robots with simple sensors},
  year = {2005},
  pages = {671--676},
  abstract = {This paper presents a distributed particle filter algorithm for localizing
	multiple mobile robots that are equipped only with low cost/low power
	sensors. This method is applicable to multi-micro robot systems,
	where size limitations restrict sensor selection (e.g. small infrared
	range finders). Localization of three robots in a known environment
	is conducted by combining measurements from a small number of simple
	range sensors with inter-robot distances obtained through an acoustic
	range finder system. The localization problem is formulated as estimating
	the global position and orientation of a single triangle, where corners
	of the triangle represent the positions of robots. The robot positions
	relative to the centroid of the triangle are then determined by trilateration
	using the inter-robot distance measurements. Each robot uses an identical
	particle filter algorithm to estimate the global position of the
	triangle. The best estimates determined by each particle filter are
	distributed among the robots for use in the following iteration.
	Simulations demonstrate the ability to perform global localization
	of three robots, each using a compass and two range finders. The
	results illustrate that this method can globally localize the robot
	team in a simulated indoor environment. The results are compared
	to simulations where robots have access to only their own sensor
	data, which are unable to successfully localize under equivalent
	conditions.},
  booktitle = {IEEE/RSJ International Conference on Intelligent Robots and Systems},
  categories = {sensors},
  citation = {4},
  keywords = {localization, multi-robot, triangulation, particle filter, low-cost,
	simple-sensors},
  no = {009},
  review = {This paper focuses a method to perform global localization of a group
	of three robots within a known map of their environment. Firstly
	this paper discuss about numerous investigated methods of localizing
	robots by others. All other methods are beyond the capabilities of
	the small robot because of using GPS, scanning lasers, and cameras.
	In this paper, the robots use two fixed range sensors (IR), a compass
	to sense orientation, two range finders to measure the distance between
	pairs of robots. The sensors are limited in size, power, and number.
	The main purpose is to share limited sensory data and computational
	resources among the robots for a better estimation. In order to estimate
	all robot positions, each robot uses particle filter, which is described
	deeply in that paper. That is, each robot estimates its own place
	using only data from its own sensors and then share this data with
	other robots. Now, each robot has sensors readings from all robots
	information to combine the sensor readings using distributed method
	which is described to share best estimates. Simulation results are
	shown for position and sensors error on the graphs. Results show
	that cooperation amongst the robots is better to estimate positions
	of robots than each robot estimates its own position individually.
	Implementation of this method on real robots is missing from that
	paper. According the results, I can say that the results will not
	be changed and the method will work on the physical robots.}
}

@ARTICLE{citeulike:275098,
  author = {Priyantha, Nissanka B. and Chakraborty, Anit and Balakrishnan, Hari},
  title = {The Cricket location-support system},
  year = {2000},
  pages = {32--43},
  abstract = {This paper presents the design, implementation, and evaluation of
	Cricket, a location-support system for in-building, mobile, locationdependent
	applications. It allows applications running on mobile and static
	nodes to learn their physical location by using listeners that hear
	and analyze information from beacons spread throughout the building.
	Cricket is the result of several design goals, including user privacy,
	decentralized administration, network heterogeneity, and low cost.
	Rather than explicitly tracking user location, Cricket helps devices
	learn where they are and lets them decide whom to advertise this
	information to; it does not rely on any centralized management or
	control and there is no explicit coordination between beacons; it
	provides information to devices regardless of their type of network
	connectivity; and each Cricket device is made from off-the-shelf
	components and costs less than U.S. $10. We describe the randomized
	algorithm used by beacons to transmit information, the use of concurrent
	radio and ultrasonic signals to infer distance, the listener inference
	algorithms to overcome multipath and interference, and practical
	beacon configuration and positioning techniques that improve accuracy.
	Our experience with Cricket shows that several location-dependent
	applications such as in-building active maps and device control can
	be developed with little effort or manual configuration.},
  address = {New York, NY, USA},
  booktitle = {MobiCom '00: Proceedings of the 6th annual international conference
	on Mobile computing and networking},
  categories = {position},
  citation = {1713},
  citeulike-article-id = {275098},
  isbn = {1581131976},
  keywords = {context, cricket, indoor, location},
  no = {014},
  posted-at = {2005-08-05 14:55:13},
  priority = {2},
  publisher = {ACM Press},
  review = {This paper presents design, implementation and evaluation of a location
	support system, which is called as Cricket. It is a location support
	system which is low-cost (beacon and listener :< $10) and it also
	provides user privacy, decentralized administration, network heterogeneity.
	A beacon consists of a PIC micro-controller, a RF transmitter and
	an ultrasonic transmitter while a listener consists of identical
	controller, a RF receiver and an ultrasonic receiver with a tone
	detector. It is a cheap design hardware, but it is not designed for
	low power consumption. It is not indicated in this paper, but also
	can be designed for low power in the future. 
	
	In Cricket, speed of sound in air is the idea. It measures the one-way
	propagation time of ultrasonic signals from beacon to listener. When
	the listener gets RF signal, it is to turn on its ultrasonic receiver
	for listening ultrasonic pulse. Time difference between this pulse
	and the first RF signal bit is used to estimate the position. Most
	of the positioning systems use that method but using different techniques.
	This paper also presents ideas to minimize errors of the system.
	It solves the humidity and temperature problems with speed of sound
	by using relativeley coarse-grained location information while other
	papers cannot solve that problem. Different scenarios are assumed
	for better experiment. These experiments test boundary performance,
	different three listener inference algorithms, which are Majority,
	Min-Mean, Min-Mode, and mobile performance. It is also planned to
	measure the accuracy of the hardware in the future.
	
	In this paper, Cricket is compared with Bat, Active Badge, Radar systems.
	The benefits and limitations of these systems are discussed, beside
	that showing some similarities of them. In Bat system, there is a
	central control of the system while cricket had no central control.
	It makes it simpler, and reduces management cost. Like Bat, Active
	Badge is difficult with matrix of sensors. In other words, Active
	Badge and Bat is more expensive than Cricket. Radar uses the RF signal
	strength as an indicator of the distance between transmitter and
	receiver. It works only with one network. In conclusion, Cricket
	,which is a location support-system, is cheap, easy to implement
	. Active map and location-based services can be implemented.}
}

