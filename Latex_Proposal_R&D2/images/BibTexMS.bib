@ARTICLE{bjerknes2007lcu,
  author = {Bjerknes, J.D. and Liu, W. and Winfield, AF and Melhuish, C. and
	Lane, C.},
  title = {Low Cost Ultrasonic Positioning System for Mobile Robots},
  journal = {Proceeding of Towards Autonomous Robotic Systems},
  year = {2007},
  pages = {107--114},
  abstract = {This paper describes a simple low-cost 2D and 3D positioning system
	for mobile robots. The system makes use of both ultrasound and radio
	frequency signals to achieve positional accuracies better than 1%
	of the tracked volume. Furthermore, we have integrated the positioning
	system into the simulation tools Player/Stage, in order to combine
	simulated robots with real robots in a ?hybrid? embodied simulation.
	The paper presents measurements from using the positioning system
	for both 3D tracking and 3D closed loop control.},
  categories = {position_3d_2d_absolute},
  citation = {1},
  keywords = {low-cost 2D-3D positioning system, tracking, mobile robots, Player/Stage},
  no = {007},
  review = {This paper presents a simple design of a 2D and 3D positioning tracking
	system by using inexpensive ultrasonic sensors, radio device and
	micro-controller. In particular, the tracking system is done by integration
	of the system with Player/Stage. 
	
	The idea behind of the system is to measure time-of the flight of
	the ultrasonic signals using Trilateration method. Radio frequency
	is for clock synchronization. The system accuracy is better than
	99%. However, the system requires a fixed infrastructure which is
	disadvantage due to the fact that two or more fixed points of beacons
	should be known to determine the position of an object. After having
	at least two fixed points, and time of flight, actually finding the
	position is so simple by Heron?s equation. When there are two beacons
	only and a robot is in a position close to the line between two beacons,
	there is a huge difference from the correct position estimation.
	In fact, two beacons are sufficient for 2D positioning but in this
	paper, 8 ultrasonic beacons are used to help a robot to calculate
	its position when there are people around the robot and for also
	3D positioning. It is not indicated in that paper, the system can
	be call as complementary of GPS for indoors application. Like GPS,
	a robot calculate the position itself. This means when there is also
	more than one robot, it does not increase computational load on the
	system unlike visual tracking system. 
	
	Integration this system to Player/State provides simulations of real
	robots. It is not offered in that paper, but I offer that this integration
	can be used for educational purposes in Universities. There can be
	a room includes some special robots, and students can connect to
	this room on Internet by Player/Stage to remote the robots, so students
	or other people would have a change to program and test real robots
	from their own computers. The other missing from the paper is ignoring
	changes in temperature, because the speed of sound changes by roughly
	0.18% per Celsius. It is implied as further work , but not implemented
	by adding temperature sensors to change system?s calibration.}
}

@INPROCEEDINGS{Borenstein97mobilerobot,
  author = {J. Borenstein and H. R. Everett and L. Feng and D. Wehe},
  title = {Mobile Robot Positioning Sensors and Techniques},
  year = {1997},
  volume = {14},
  pages = {231-249},
  abstract = {Exact knowledge of the position of a vehicle is a fundamental problem
	in mobile robot applications. In search for a solution, researchers
	and engineers have developed a variety of systems, sensors, and techniques
	for mobile robot positioning. This paper provides a review of relevant
	mobile robot positioning technologies. The paper defines seven categories
	for positioning systems: 1. Odometry; 2. Inertial Navigation; 3.
	Magnetic Compasses; 4. Active Beacons; 5. Global Positioning Systems;
	6. Landmark Navigation; and 7. Model Matching. The characteristics
	of each category are discussed and examples of existing technologies
	are given for each category. The field of mobile robot navigation
	is active and vibrant, with more great systems and ideas being developed
	continuously. For this reason the examples presented in this paper
	serve only to represent their respective categories, but they do
	not represent a judgment by the authors. Many ingenious approaches
	can be found in the literature, although, for reasons of brevity,
	not all could be cited in this paper.},
  categories = {system},
  citation = {209},
  journal = {Invited paper for the Journal of Robotic Systems, Special Issue on
	Mobile Robots. No. 4,},
  keywords = {Odometry, Inertial Navigation, Reference-based systems, Magnetic Compasses,
	Active Beacons, Global Positioning Systems, Landmark Navigation,
	Model Matching},
  no = {020},
  review = {This paper reviews the state of the art in sensors, systems, methods
	and technologies for mobile robot positioning. It defines and categorizes
	systems without any judgments. This paper uses the results and specifications
	from other papers, In other words, systems are not tested by the
	authors. The systems are Odometry, Inertial Navigation, Magnetic
	Compasses, Active Beacons, Global Positioning Systems, Landmark Navigation,
	Model Matching. As first relative system, odometry is the most popular
	one which is inexpensive, and it provides good short-term accuracy.
	This paper also points some methods to measure odometry errors. As
	a second relative system, inertial navigation sensors are described.
	First inertial sensor is accelerometers and tilt sensor supplies
	information to accelerometers to cancel gravity component. More about
	INS is about gyroscopes. It is normally so expensive but recent technology
	provides fiber-optic gyros (laser gyros) which is inexpensive. Many
	magnetic compasses are presented. This paper choose the best compass
	type as fluxgate compasses for mobile robots, because of low power
	consumption, and low cost. Technical specifications for that compass
	is shown. As absolute position system, trilateration and triangulation
	methods are described, and as summery of this, this paper indicates
	that using more than one method helps overcome the individual weaknesses.
	As a next system, GPS and its errors and history is covered. GPS
	calculates latitude, longitude and altitude of a position by using
	time of the flight of satellites' RF signals. Furthermore, landmark
	navigation is described. A robot can recognize the landmarks' distinct
	features such as geometric shapes; rectangles, lines, circles which
	are extracted by using sensors. It addresses two kinds of landmarks:
	artificial and natural and summarize the characteristics of them.
	I conclude that landmark is inexpensive, flexible systems than others.
	Similarly to landmark and as last system in that paper, map-base
	positioning matches local map with the map previously stored in the
	environment. This paper addresses the problem that mobile robot still
	needs universal indoor navigation method despite the many existing
	systems. It is nice to point that problem to challenge researchers.
	It was written 1997, still there is not such a universal system which
	works accurately and which is inexpensive. Like every survey, this
	survey paper has a comparison table which shows the features, accuracy
	and range of positioning systems described in that paper, therefore
	we can analyze them easily on that table.}
}

@ARTICLE{Bulusu00gps-lesslow,
  author = {Nirupama Bulusu and John Heidemann and Deborah Estrin},
  title = {GPS-less Low Cost Outdoor Localization For Very Small Devices},
  year = {2000},
  volume = {7},
  pages = {28-34},
  abstract = {Instrumenting the physical world through large networks of wireless
	sensor nodes, particularly for applications like environmentalmonitoring
	of water and soil, requires that these nodes be very small, light,
	Pactical considerations such as the small size, form factor, cost
	and power constraints of nodes preclude the reliance on GPS (Global
	Positioning System) on all nodes in these networks. In this paper,
	we review localization techniques and evaluate the effectiveness
	of a very simple connectivity-metric method for localization in outdoor
	environments that makes use of the inherent radio-frequency (RF)
	communications capabilities of these devices. A fixed number of reference
	points in the network with overlapping regions of coverage transmit
	periodic beacon signals. Nodes use a simple connectivity metric,that
	is more robust to environmental vagaries, to infer proximity to a
	given subset of these reference points. Nodes localize themselves
	to the centroid of their proximate reference points. The accuracy
	of localization is then dependent on the separation distance between
	two adjacent reference points and the transmission range of these
	reference points. Initial experimental results show that the accuracy
	for 90% of our data points is within one-third of the separation
	distance. However future work is needed to extend the technique to
	more cluttered environments.},
  categories = {navigation},
  citation = {1345},
  howpublished = {Personal Communications, IEEE},
  keywords = {localization, location, radio-frequency wireless network},
  no = {001},
  review = {This paper addresses the problem of localization which is about determining
	the position of a mobile robot or any devices. The main goal is to
	demonstrate effectiveness of a localization algorithm which use idealized
	radio model in unconstrained outdoor environments. 
	
	First of all, it presents localization approaches by showing their
	positive and negative sides. Classification of localization approaches
	is well defined in this paper. The first approach is fine-grained
	localization which includes range-finding , directionality based
	and signal matching methods. One of the key ideas is measuring the
	time of flight of the communication signal. It can be GPS which measures
	one-way flight or LPS (Pinpoint's Local Positioning System) which
	measures round-trip time. However, GPS is not practical, expensive
	and use a lot of energy. LPS is for most indoor applications. Like
	LPS, Ultrasound systems may not work in outdoors because of interference
	from ultrasound resources. Another key idea is signal strength which
	is the attenuation of the radio signal, but extensive infrastructural
	effort is required and it may be a good idea for indoors if can be
	achieved to measure signal strength by applying a Wall attenuation.
	Another idea is signal pattern matching which analyzes signal structure
	and store in its database instead of signal timing and strength,
	but also an extensive effort is needed with this idea. The another
	one is directionality which compute the angle of each reference points
	with respect to the mobile node by using a complex antenna array
	and which is costly, so this is not preferable. By the way, it is
	not very efficient in indoors because of multipath effects. The last
	idea is active badge system which was one of the systems for indoor
	environments using IR technology. IR sensors are physically wired
	in rooms of a building .It requires significant installation, and
	configuration and IR is affected by sunlight. Therefore it cannot
	be used outdoors. 
	
	This paper gets many ideas from previous works. It uses RF-based localization
	and measures signal strength of received beacon signals to estimate
	the distance. Some definitions and formulas are given, which are
	pretty clear. For these formulas, it is assumed that perfect spherical
	radio propagation is used and all radios are identical. They both
	test their system in indoors and outdoors. Like some systems, they
	are not successful in indoors because of walls. They use very simple,
	so very cheap model but it works with 87% accuracy in outdoors, and
	very effective .The good job is for experimental results, they test
	their model many times by changing the distances and they give persuasive
	results.
	
	The paper also well outlines future work. This paper was published
	in 2000, For 8 years, electronics technology has been improved a
	lot, so nowadays this kind of model can be implemented with more
	accurately and cheaper without having many problems such as the collision
	avoidance problem in this paper.}
}

@ARTICLE{Changsong04remoteeyes:a,
  author = {Ubiquitous Computing Changsong and Changsong Shen and Baosheng Wang
	and Florian Vogt and Steve Oldridge and Sidney Fels},
  title = {RemoteEyes: A Remote Low-Cost Position Sensing Infrastructure for
	Ubiquitous Computing},
  year = {2004},
  pages = {31--35},
  abstract = {This paper describes the architecture of a remote low-cost position
	sensing infrastructure for ubiquitous computing. To reduce the cost,
	each tracked object carries an inexpensive active tag that emits
	a modulated near-infrared signal, which is received and decoded by
	a camera module. Each camera module consists of a CMOS camera connected
	to a Field Programmable Gate Array (FPGA) and Ethernet communication
	hardware. Remote sensing can be achieved by means of Ethernet. The
	use of inexpensive FPGA to implement the most time- and data-critical
	tracking algorithms provides a costeffective platform with low latency.
	The preliminary results suggest it is successful at tracking objects
	and feasible for achieving medium fidelity tracking performance in
	both space and time.},
  booktitle = {University of Tokyo, Japan},
  categories = {position},
  citation = {8},
  keywords = {position, sensing, network, FPGA, ubiquitous computing,low cost},
  no = {013},
  review = {In this paper, a remote low-cost position sensing system is presented.
	Multiple low-cost camera modules are used to track inexpensive infrared
	tags. There is more than one camera module, because of 3D positioning.
	Each camera module is integrated with FPGA technology (Altera?s low
	cost Cyclone EP1C12). The system can truck multiples tags, because
	each tag has a unique identifier. The system meets demands of low-cost,
	low-latency, medium fidelity and remote accessible infrastructure.
	However, this system requires a host computer .The host computer
	and FPGA/Scale are connected to each other by Ethernet, so this paper
	ignore the cost of that host computer. 
	
	
	This paper criticizes other available systems basically such as GPS,
	wireless enhanced , RADAR, and Active-badge. The other systems may
	estimate the position precisely, but some of them are expensive systems,
	some of them are not for indoors. In this paper, the system is for
	indoor applications. It describes the system architecture, and processing
	step which includes some algorithms for different problems. These
	processing pipelines are blinking pattern encoding, raw image capturing,
	bright spot detection and grouping, invalid bright-spot removal,
	blinking pattern decoding and 3D coordinate transformation. Nevertheless,
	they are told here briefly like telling a story. If someone wish
	to do this kind of system, this algorithms? names will be keywords
	for that person. Furthermore, it is mentioned about the features
	of the boards and processors, it is not necessary to tell them, therefore
	it is useless. For the future, it is planned to evaluate the precision
	of this system in details.}
}

@ARTICLE{10.1109/VSMM.2001.969693,
  author = {Jaeyong Chung and Namgyu Kim and Gerard Jounghyun Kim and Chan-Mo
	Park},
  title = {POSTRACK: A Low Cost Real-Time Motion Tracking System for VR Application},
  journal = {Virtual Systems and MultiMedia, International Conference on},
  year = {2001},
  volume = {0},
  pages = {383},
  abstract = {One of the obstacles to the proliferation the VR technology to digital
	contents is the expensive, intrusive, cumbersome and brittle nature
	of the sensors that are required to detect user's intent. While optical
	tracking, being wireless with respect to the user's body, has been
	regarded as one solution to this usability aspect, the traditional
	problems of establishing marker correspondence and resolving their
	occlusions in real time still remain. One avenue of efforts to address
	these problems is to simply add more and more hardware (or processing
	power), making the tracking system too expensive for general usage,
	while another major effort looks to directly track human body parts,
	but usually suffers from the inability to track point features. In
	this paper, we present a relatively inexpensive (e.g. runs on a high-end
	PC), but reasonably accurate real time optical motion tracking system,
	called the 'POSTRACK', that can still find a wide range of applications
	for VR. POSTRACK uses four cheap 8-bit grayscale cameras attached
	with infrared LED?s, and the user wears several (1~5) highly reflective
	markers. The markers are ?designed? to be very easy to wear (snap-on),
	and even fashionable. The four cameras are calibrated using well
	known algorithms, and the initial marker assignments are found by
	a simple heuristic based on the normal human posture, while the fundamental
	matrices are used to find blob correspondence and compute the 3D
	positions of the markers. Since the snap-on markers are rather large
	and their positions are computed from their image centroids (which
	constantly change), the computed 3D position data are jittery, more
	so than other trackers that use much smaller marker sizes, or compared
	to magnetic trackers. While the technology is old (i.e. uses standard
	vision and stereo algorithms), the engineering of the POSTRACK strikes
	a good balance between cost and its capability, and we demonstrate
	this through results from using it for gesture motion recognition
	and orientation tracking for 3D pointing.},
  address = {Los Alamitos, CA, USA},
  categories = {vision},
  citation = {12},
  isbn = {0-7695-1402-2},
  keywords = {POSTRACK, motion tracking, low-cost , Gesture Motion Recognition},
  no = {018},
  publisher = {IEEE Computer Society},
  review = {In this paper, a real time optical motion tracking system, and its
	applications are presented. This system is compared with another
	system to show system performance. Even though it is not written
	about robotics applications, it can be used for robotics applications
	also. As materials, four cheap 8-bit grayscale cameras (with infrared
	LEDs) and markers are used. The markers are worn by a user, which
	are highly reflective. This system requires an infrastructure such
	as mounting cameras to ceiling and connecting the cameras to a computer,
